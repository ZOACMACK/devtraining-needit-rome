        <script><![CDATA[var CZ_NeedIt_Ajax = Class.create();
CZ_NeedIt_Ajax.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	
	// get the E-Mail-Address from User Record based on provided sys_id of User
	getEmailFromUserID: function() {
		var grUser = new GlideRecord('sys_user');
		grUser.get(this.getParameter('sysparm_sUserID'));
		return grUser.email+'';
	},
	
    type: 'CZ_NeedIt_Ajax'
});]]></script>
